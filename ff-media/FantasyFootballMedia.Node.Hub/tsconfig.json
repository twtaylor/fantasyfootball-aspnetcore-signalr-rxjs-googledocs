{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "system",
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "outFile": "./Scripts/tsc.js"
  },
  "files": [
    "app.ts"
  ],
  "include": [
    "Helpers/*",
    "Model/*"
  ], 
  "exclude": [
    "node_modules"
  ]
}
